pipeline {
   agent any
   environment {
	ALL = 'all'
	BASH = 'bash'
	PYTHON = 'python'
	C = 'c'
	
   }

   stages {
      stage('python') {
         steps {
            cat python
         }
      }
	  stage('bash') {
         steps {
            cat bash
         }
      }
	  stage('c') {
         steps {
            cat c
         }
      }
   }
}
