pipeline {
   agent {lable 'slave01'}
   environment {
	ALL = 'all'
	BASH = 'bash'
	PYTHON = 'python'
	C = 'c'
	
   }

   stages {
      stage('python') {
         steps {
            sh 'cat python'
         }
      }
	  stage('bash') {
         steps {
            sh 'cat bash'
         }
      }
	  stage('c') {
         steps {
            sh 'cat c'
         }
      }
   }
}
